generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

type NewrestaurantsListings {
  discountedPrice Int
  originalPrice   Int
  quantity        Int
  title           String
}

model Newrestaurants {
  id             String                   @id @default(auto()) @map("_id") @db.ObjectId
  averageRating  String
  description    String
  displayAddress String
  /// Multiple data types found: String: 86.7%, Float: 13.3% out of 1000 sampled entries
  imageUrl       Json
  latitude       String
  listings       NewrestaurantsListings[]
  longitude      String
  name           String
  passwordHash   String
  /// Multiple data types found: String: 97.4%, Float: 2.6% out of 1000 sampled entries
  priceRange     Json
  ratingCount    String
  username       String
}
